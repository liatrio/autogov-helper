{
  "artifact": {
    "version": "{{ .Version }}",
    "created": "{{ .Created }}",
    "type": "{{ .Type }}",
    "registry": "{{ .Registry }}",
    "fullName": "{{ .FullName }}",
    "digest": "{{ .Digest }}",
    "path": "{{ .Path }}"
  },
  "metadata": {
    "buildType": "{{ .BuildType }}",
    "permissionType": "{{ .PermissionType }}",
    "repository": {
      "name": "{{ .Repository }}",
      "owner": "{{ .Owner }}",
      "url": "{{ .GitHubServerURL }}"
    },
    "workflow": {
      "name": "{{ .WorkflowName }}",
      "ref": "{{ .WorkflowRefPath }}",
      "id": "{{ .RunID }}"
    },
    "job": {
      "name": "{{ .JobName }}",
      "id": "{{ .RunID }}"
    },
    "runner": {
      "name": "{{ .RunnerName }}",
      "os": "{{ .OS }}"
    },
    "commit": {
      "sha": "{{ .SHA }}",
      "message": "{{ .CommitMessage }}",
      "author": "{{ .CommitAuthor }}",
      "url": "{{ .CommitURL }}"
    }
  }
} 